{
  "expo": {
    "name": "wilo-smart-pump-operator",
    "slug": "wilo-smart-pump-operator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wilo.svg",
    "scheme": "wilosmartpumpoperator",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wilo.svg",
        "backgroundColor": "#009C82"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#009C82"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
